{
  "compilerOptions": {
    "target": "es2015",
    "lib": ["dom", "dom.iterable", "es6", "es2015"],
    "downlevelIteration": true,
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "allowImportingTsExtensions": false,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"],
      "@/components/*": ["./src/components/*"],
      "@/lib/*": ["./src/lib/*"],
      "@/types/*": ["./src/types/*"],
      "@/content/*": ["./content/*"]
    }
  },
  "include": [
    ".next/types/**/*.ts",
    "build-output/types/**/*.ts",
    "build/types/**/*.ts",
    "fresh-build/types/**/*.ts",
    "next-env.d.ts",
    "out/types/**/*.ts",
    "src/**/*.ts",
    "src/**/*.tsx",
    "src/test/types.d.ts",
    "build-1760027167401/types/**/*.ts"
  ],
  "exclude": ["node_modules", "Zone_*/**/*", "Zone_*", "*.zip"]
}
