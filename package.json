    "s3:validate": "node scripts/validate-s3-infrastructure.js",
    "s3:test": "node scripts/test-s3-infrastructure.js",
    "infrastructure:setup": "node scripts/setup-infrastructure.js",
    "deploy:s3": "node scripts/deploy.js",
    "rollback:backup": "node scripts/rollback.js backup",
    "rollback:list": "node scripts/rollback.js list",
    "rollback:emergency": "node scripts/rollback.js emergency",
